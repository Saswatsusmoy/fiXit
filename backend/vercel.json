{
    "builds": [
      {
        "src": "./run.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "build": {
      "env": {
        "PYTHON_VERSION": "3.9"
      }
    },
    "devCommand": "python run.py",
    "buildCommand": "pip install -r requirements.txt"
  }
  